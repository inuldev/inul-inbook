PORT=8000
MONGO_URI=your_mongodb_connection_string
JWT_SECRET=your_jwt_secret
JWT_EXPIRES_IN=30d
# This will need to be updated with your actual frontend URL after deployment
FRONTEND_URL=https://your-frontend-vercel-url.vercel.app
NODE_ENV=production
SESSION_SECRET=your_session_secret

# Cloudinary credentials
CLOUDINARY_NAME=your_cloudinary_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Google OAuth credentials
# Note: You'll need to update the callback URL in Google Developer Console
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# This will need to be updated with your actual backend URL after deployment
GOOGLE_CALLBACK_URL=https://your-backend-vercel-url.vercel.app/api/auth/google/callback

# Logging
LOG_LEVEL=error
